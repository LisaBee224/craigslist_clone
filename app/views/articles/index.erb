<div class="container">
  <ul>
    <%@articles.each do |article| %>
    <li><%=article.title%></li>
    <li><%=link_to 'show', article_path(article) %>
    <li><%=link_to 'edit', edit_article_path(article) %>
    <li><%=link_to 'delete', article_path(article),
              method: :delete,
              data: {confirm: 'you sure?'}%></li>
              <%end%>

  </ul>

</div>
